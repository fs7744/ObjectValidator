<configuration>
  <configSections>
    <section name="nair" type="Nair.Sdk.NairConfiguration, Nair.Sdk"/>
  </configSections>
  <nair defaultCluster="myCluster"
        devPortalUrl="http://10.16.75.10:9091"> <!--Developer Portal Address-->
    <clusters>
      <!--
        nairserverips: multiple nair servers split with ","
        databaselocation: location(WH7, E3, E4) which the nair servers handle.

        GQC Nair: 172.16.12.154:8887,172.16.12.155:8887
      -->
      <add name="myCluster"
           nairserverips="10.16.75.25:8887,10.16.75.26:8887,10.16.75.27:8887"  
           databaselocation="WH7" /> 
    </clusters> 
  </nair>  
</configuration>